{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"outDir": "./dist",
		"strict": true ,
		"noFallthroughCasesInSwitch": true, // switch文で、caseをbreakやreturnで終えていることを必須
		"forceConsistentCasingInFileNames": true, // ファイルの文字列の大文字小文字を区別
	}
}
